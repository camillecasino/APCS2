import java.io.*;
import java.util.*;

public class Knights {

    public Knights(int x, int y) {
	
	int px1, px2, px3, px4, py1, py2, py3, py4;

        px1 = x + 3;
	px2 = x + 1;
	px3 = x - 3;
	px4 = x - 1;
    }

    public int getPx1() {
	return this.px1;
    }

    public static void main(String[] args) {
	Knights k = new Knights(3,4);
	System.out.println(k.getPx1());
    }
}
