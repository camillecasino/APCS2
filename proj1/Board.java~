import java.io.*;
import java.util.*;

public class Board {
    
    char[][] b;
    int maxX;
    int maxY;

    public Board() {
	maxX = 5;
	maxY = 5;

	b = new char[maxX][maxY];
	for (int i = 0; i < maxX; i++) {
	    for (int j = 0; j < maxY; j++) {
		b[i][j] = ".";
	    }
	}
    }

    public static void main(String[] args) {
